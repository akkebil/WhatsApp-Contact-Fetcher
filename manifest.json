{
  "manifest_version": 2,
  "name": "WhatsApp Contact Fetcher",
  "version": "1.0",
  "author": "Ahmet Bilir",
  "description": "Retrieves a list of contacts from a chat group. By: A Bilir",
  "permissions": ["activeTab", "downloads"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon16.png",
      "128": "icon16.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon16.png",
    "128": "icon16.png"
  }
}
